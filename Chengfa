package game;

import java.util.Scanner;

public class asd {
	public static void main(String[] args) {	
	String[][] save = new String[10][10];	
	Scanner input  = new Scanner(System.in);
	System.out.println("请输入乘法表行数：");
	int n = input.nextInt();
	for (int i = 1; i <=n; i++) {
		for (int j = 1; j <=i; j++) {
			save[i][j] = i+"*"+j+"="+(i*j);
			System.out.print(save[i][j]+" " );
		}
		System.out.println("");
		}
	}
}
